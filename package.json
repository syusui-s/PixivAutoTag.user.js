{
  "name": "pixiv-auto-tag",
  "version": "2.0.0",
  "description": "Pixivでブックマークタグ付け自動化してくれるUserScriptを書いたぞ！！",
  "main": "src/main.mjs",
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "rollup --config",
    "build-test": "rollup --config rollup.test.config.js",
    "lint": "eslint src/**/*.js",
    "fix": "eslint --fix src/**/*.js test/**/*.js",
    "test": "npm run build-test && mocha --require source-map-support/register build/test/**/*.js && npm run lint",
    "watch": "mocha --require ts-node/register --watch build/test/**/*.js",
    "typecheck": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syusui-s/PixivAutoTag.user.js.git"
  },
  "author": "Shusui MOYATANI",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/syusui-s/PixivAutoTag.user.js/issues"
  },
  "homepage": "https://github.com/syusui-s/PixivAutoTag.user.js#readme",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.7",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "chokidar": "^3.3.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "license-checker": "^25.0.1",
    "mocha": "^6.2.2",
    "prettier": "^1.19.0",
    "rollup": "^1.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "hyperapp": "^1.2.10",
    "hyperx": "^2.5.4"
  }
}
